{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "ConnectionStrings": {
    "AquariumDB": "Data Source=.;Initial Catalog=Aquarium;Integrated Security=True;MultipleActiveResultSets=True;App=AquariumApi"
  },
  "Tokens": {
    "Issuer": "https://aquariumonitor.azurewebsites.net",
    "Audience": "https://aquariumonitor.azurewebsites.net",
    "ValidForDurationMins": "30"
  },
  "Cors": {
    "PolicyName": "AquariumMonitor",
    "Origins": "aquariumonitor.azurewebsites.net"
  }
}
